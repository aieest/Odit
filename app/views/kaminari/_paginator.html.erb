<%= paginator.render do -%>
  <nav class="pagination" role="navigation" aria-label="pager">
    <%= prev_page_tag unless current_page.first? %>
    <% each_page do |page| -%>
      <% if page.display_tag? -%>
        <%= page_tag page %>
      <% end -%>
    <% end -%>
    <%= next_page_tag unless current_page.last? %>
  </nav>
<% end -%>