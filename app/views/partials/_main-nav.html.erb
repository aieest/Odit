<% if current_user.profile && current_user.profile.complete? %>
    <%= link_to home_path, class: "main-nav-btn #{current_page?(home_path) ? 'active-nav-btn' : ''}" do %>
        <%= heroicon "home", variant: :outline %>
    <% end %>
    <%= link_to profile_earnings_path(current_user.profile), class: "main-nav-btn #{controller_name == 'earnings' ? 'active-nav-btn' : ''}" do %>
        <%= heroicon "plus-circle", variant: :outline %>
    <% end %>
    <%= link_to profile_expenses_path(current_user.profile), class: "main-nav-btn #{controller_name == 'expenses' ? 'active-nav-btn' : ''}" do %>
        <%= heroicon "minus-circle", variant: :outline %>
    <% end %>
    <!--
    <%= link_to 'Profile', profile_path(current_user.profile), class: "main-nav-btn" %>
    <%= link_to 'Edit Profile', edit_profile_path(current_user.profile), class: "main-nav-btn" %>
    <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: "main-nav-btn" %>
    -->
    <i class="main-nav-btn"><%= heroicon "presentation-chart-line", variant: :outline %></i>
    <%= link_to settings_path, class: "main-nav-btn #{current_page?(settings_path) ? 'active-nav-btn' : ''}" do %>
        <%= heroicon "cog-6-tooth", variant: :outline %>
    <% end %>
<% else %>
    <!--Disabled nav links-->
    <i class="main-nav-btn"><%= heroicon "home", variant: :outline %></i>
    <i class="main-nav-btn"><%= heroicon "plus-circle", variant: :outline %></i>
    <i class="main-nav-btn"><%= heroicon "minus-circle", variant: :outline %></i>
    <i class="main-nav-btn"><%= heroicon "presentation-chart-line", variant: :outline %></i>
    <i class="main-nav-btn"><%= heroicon "cog-6-tooth", variant: :outline %></i>
<% end %>